{
  "name": "@swishjam/react",
  "version": "0.0.13",
  "description": "Provides the React components and hooks to install and interact with SwishjamJS",
  "type": "module",
  "main": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types",
  "files": [
    "dist/*",
    "README.md"
  ],
  "scripts": {
    "rollup": "rollup -c rollup.config.js",
    "vite": "vite build",
    "build": "rm -rf dist && mkdir dist &&  babel ./index.js -d dist --presets=@babel/preset-env,@babel/preset-react --plugins=@babel/plugin-syntax-dynamic-import",
    "buildOLD": "rm -rf dist/ && webpack --mode production --config webpack.config.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@swishjam/core": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.22.19",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.78.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "vite": "^4.4.9"
  },
  "peerDependencies": {
    "@swishjam/core": "*",
    "react": ">=16.0.0"
  }
}
